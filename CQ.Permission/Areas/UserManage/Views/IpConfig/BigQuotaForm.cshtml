
@{
    ViewBag.Title = "BigQuotaForm";
    Layout = "~/Views/Shared/_Form.cshtml";
}

<script>
    var keyValue = $.request("keyValue");
    $(function () {

    });
    function submitForm() {
        if (!$('#form1').formValid()) {
            return false;
        }
        var postData = $("#form1").formSerialize();
        $.submitForm({
            url: "/UserManage/IpConfig/SubmitForm",
            param: postData,
            success: function () {
                $.currentWindow().$("#gridList").trigger("reloadGrid");
            }
        });
    }
</script>
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle">用户ID</th>
                    <td class="formValue">
                        <input id="UserAccount" name="UserAccount" type="text" class="form-control required" placeholder="用户ID" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">MAC地址</th>
                    <td class="formValue">
                        <input id="IPAddress" name="IPAddress" type="text" class="form-control required" placeholder="MAC地址" />
                    </td>

                </tr>
            </table>
        </div>
    </div>
</form>

