
@{
    ViewBag.Title = "游戏中心-棋牌-斗地主,麻将,牛牛,棋牌游戏平台";
    Layout = "~/Views/Shared/_LayoutGame.cshtml";
}
<div class="wrap mt20">
    <div class="hd">
        <h3><i class="icon-title icon-event"></i>游戏中心</h3>
    </div>
    <div class="game-app clearfix">
        <ul id="GameList">
            
        </ul>
    </div>


</div>
<script>
    $(function() {
        GetList();
    });

    function GetList() {
        var keyValue = 0;
        $.ajax({
            url: "/Game/GetListJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                var _html = "";
                $.each(data, function (i, o) {
                    _html += '<li>';
                    _html += '  <a href="/Game/Game?keyValue=' + o.F_Id + '" title="' + o.F_ProductName + '" class="app-icons">';
                    _html += '    <img src="http://183.131.69.236:11010'+o.F_IcoImg+'" alt="">';
                    _html += '    <h4>' + o.F_ProductName + '</h4>';
                    _html += '  </a>';
                    _html += '</li>';
                });
                $("#GameList").prepend(_html);
            },
            error: function (msg) {
                alert(msg.statusText);
            }
        });
    }

</script>

